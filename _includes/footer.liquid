                {% if (base_dir[1] == "2018") %}

                {% else %}
                    <ul style="padding-left:0px;">
                    <li>
                        <a href="https://www.deutsche-stiftung-engagement-und-ehrenamt.de/" target="_blank">
                            <img style="width: 60%" src="{{ site.baseurl }}/assets/img/static/D-S-E-E.png"/>
                        </a>
                    </li>
                    <li>

                        <a href="https://okfn.de/" target="_blank">
                            <img style="width: 50%" src="{{ site.baseurl }}/assets/img/static/logo-okfn.png"/>
                        </a>
                    </li>
                    <li>

                        <a href="https://open-hardware-allianz.de/" target="_blank">
                            <img style="width: 80%" src="{{ site.baseurl }}/assets/img/static/logo_oha.png"/>
                        </a>
                    </li>
                    </ul>
                {% endif %}
            </div>
            <div class="sponsors-nav">

              <div lass="cc-info">
                  <a rel="license" href="http://creativecommons.org/licenses/by/4.0/"><img alt="Creative Commons License" style="border-width:0" src="https://raw.githubusercontent.com/fablabcb/fablabcb.github.io/master/assets/img/static/cc-by88x31.png" /></a><br/><p class="bold">Inhalte dieser Webseite sind, sofern nicht anders angegeben, nach <a rel="license" href="http://creativecommons.org/licenses/by/4.0/">Creative Commons 4.0 Attribution lizenziert.</a>
